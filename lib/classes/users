//import 'package:flutter/material.dart'

import 'dart:ffi';

class Users {

  Users(True,theo,jouve,fontaine,motdepasse);

  Int? age;
  Bool? isAdmin;
  String? name;
  String? fname;
  String? surname;
  String? mdp;
  //String? email;

  User(Bool isAdmin, Int age, String name, String fname, String surname, String mdp) {
    this.isAdmin=isAdmin;
    this.age=age;
    this.name=name;
    this.fname=name;
    this.surname=surname;
    this.mdp=mdp;
    //this.email=email;
    
  }
}